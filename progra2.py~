archivo=input()



def retornar_contenido_arch(nombre_archivo):#da los contenidos como un string
    "retorna el texto"
    manejador_arch=open(nombre_archivo, encoding="utf-8")
    contenidos=manejador_arch.read()
    print (contenidos)

